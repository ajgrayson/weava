.panel.panel-default{:data => { :page => "desk_sync_progress", :job_id => @job_id }}
  .panel-heading
    %h3.panel-title 
      Setting up New Desk Project
  .panel-body
    %p.status-message.muted
      = @sync_status_message
    .progress.progress-striped.active
      .progress-bar.progress-bar-info{:role => "progressbar", :data => {:valuenow => @sync_progress_value.to_s, :valuemin => "0", :valuemax => "100"}, :style => "width: " + @sync_progress_value.to_s + "%"}

    -# .pull-right
