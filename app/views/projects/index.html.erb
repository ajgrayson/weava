<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="pull-left panel-title">Projects</h3>
    <%= link_to 'New', new_project_path, :class => "pull-right" %>
    <div class="clearfix"></div>
  </div>
  <div class="panel-body">
  <table class="table">
  <thead>
    <tr>
    <th>Name</th>
    <th>Path</th>
    </tr>
    </thead>
    <tbody>
  <% @projects.each do |project| %>
    <tr>
        <td>
            <%= link_to project.name, project_path(project) %>
        </td>
        <td>
            <%= project.path %>
        </td>
    </tr>
  <% end %>
  </tbody>
  </table>
  </div>
</div>